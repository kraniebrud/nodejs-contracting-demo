            "post": {
                "operationId": "createPerson",
                "description": "creates a new person",
                "tags": [
                    "persons"
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "additionalProperties": false,
                                "properties": {
                                    "firstName": {
                                        "type": "string",
                                        "example": "Ricky",
